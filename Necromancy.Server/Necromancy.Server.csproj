<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>netstandard2.1</TargetFramework>
        <RootNamespace>Necromancy.Server</RootNamespace>
        <Title>Necromancy Server</Title>
        <Company>Necromancy Team</Company>
        <Product>Necromancy.Server</Product>
        <Version>$(Version)</Version>
        <Copyright>Copyright © 2019 Necromancy Team</Copyright>
        <LangVersion>8</LangVersion>
    </PropertyGroup>
    <ItemGroup>
        <PackageReference Include="Arrowgene.Services" Version="1.12.1" />
        <PackageReference Include="BCrypt.Net-Next.StrongName" Version="3.1.3" />
        <PackageReference Include="MySqlConnector" Version="0.56.0" />
        <PackageReference Include="System.Data.SQLite.Core" Version="1.0.112" />
    </ItemGroup>
    <ItemGroup>
        <ContentWithTargetPath Include="Database\Script\**">
            <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
            <TargetPath>Database\Script\%(RecursiveDir)%(Filename)%(Extension)</TargetPath>
        </ContentWithTargetPath>
        <ContentWithTargetPath Include="Client\**">
            <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
            <TargetPath>Client\%(RecursiveDir)%(Filename)%(Extension)</TargetPath>
        </ContentWithTargetPath>
    </ItemGroup>
    <ItemGroup>
      <_UnmanagedRegistrationCache Remove="obj\Necromancy.Server.csproj.UnmanagedRegistration.cache" />
    </ItemGroup>
    <ItemGroup>
      <_ResolveComReferenceCache Remove="obj\Debug\netstandard2.1\Necromancy.Server.csproj.ResolveComReference.cache" />
    </ItemGroup>
    <ItemGroup>
      <IntermediateAssembly Remove="obj\Debug\netstandard2.1\Necromancy.Server.dll" />
    </ItemGroup>
    <ItemGroup>
      <_DebugSymbolsIntermediatePath Remove="obj\Debug\netstandard2.1\Necromancy.Server.pdb" />
    </ItemGroup>
    <ItemGroup>
      <_DeploymentManifestEntryPoint Remove="obj\Debug\netstandard2.1\Necromancy.Server.dll" />
    </ItemGroup>
    <ItemGroup>
      <ApplicationManifest Remove="obj\Debug\netstandard2.1\Native.Necromancy.Server.manifest" />
    </ItemGroup>
    <ItemGroup>
      <DeployManifest Remove="obj\Debug\netstandard2.1\Necromancy.Server.application" />
    </ItemGroup>
    <ItemGroup>
      <ClsidMap Remove="obj\Debug\netstandard2.1\Necromancy.Server.clsidmap" />
    </ItemGroup>
    <ItemGroup>
      <ContentWithTargetPath Remove="Client\Data\Settings\TempNPCData.csv" />
    </ItemGroup>
    <ItemGroup>
      <RegFreeComManifest Remove="obj\Debug\netstandard2.1\Necromancy.Server.X.manifest" />
    </ItemGroup>
</Project>
