    "version": "0.2.0",
    "configurations": [
        {
            "name": "Interactive",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/Necromancy.Cli/bin/Debug/netcoreapp3.0/Necromancy.Cli.dll",
            "args": [],
            "cwd": "${workspaceFolder}/Necromancy.Cli",
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "stopAtEntry": false
        },  {
            "name": "Server",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/Necromancy.Cli/bin/Debug/netcoreapp3.0/Necromancy.Cli.dll",
            "args": ["server", "start"],
            "cwd": "${workspaceFolder}/Necromancy.Cli",
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "stopAtEntry": false
        },  {
            "name": "Server --no-data",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/Necromancy.Cli/bin/Debug/netcoreapp3.0/Necromancy.Cli.dll",
            "args": ["server", "start", "--no-data"],
            "cwd": "${workspaceFolder}/Necromancy.Cli",
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "stopAtEntry": false
        }
    ]
}